    {
      "version": 1,
      "input": {
        "time_python": "time_python",
        "arduino": {
          "baudrate": 9600,
          "timeout": 3,
          "columns": [
            {
              "point": "time_arduino",
              "conversion_factor": 1E-3
            },
            {
              "point": "Sensor 1",
              "conversion_factor": 0.004887586
            },
            {
              "point": "Sensor 2",
              "conversion_factor": 0.004887586
            }
          ]
        }
      },
      "gui": {
        "window_title": "Aves Demo",
        "refresh_time_ms": 100,
        "axes": {
          "A subplot": {
            "row": 0,
            "col": 0,
            "rowspan": 1,
            "colspan": 1,
            "points": ["Sensor 1"],
            "options": {
              "ylim": [-0.5, 5.5],
              "ylabel": "Sensor 1 (V)"
            }
          },
          "Another subplot": {
            "row": 2,
            "col": 0,
            "rowspan": 1,
            "colspan": 1,
            "points": ["Sensor 2"],
            "options": {
              "ylim": [-0.5, 5.5],
              "ylabel": "Sensor 2 (V)"
            }
          }
        },
       "sharexaxis": true,
       "x_points": "time_arduino"
      },
      "output" : {
        "columns": [
          "time_python", "time_arduino", "A", "B", "Sensor 1", "Sensor 2"
        ]
      }
    }
